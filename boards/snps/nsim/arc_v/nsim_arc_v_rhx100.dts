/dts-v1/;

#include "rhx100.dtsi"

/ {
	model = "Synopsys RHX100";
	compatible = "snps,rhx100";

	aliases {
		uart-0 = &uart0;
	};

	chosen {
		zephyr,flash = &iccm0;
		zephyr,sram = &dccm0;
		zephyr,console = &uart0;
		zephyr,shell-uart = &uart0;
	};
};

&uart0 {
	status = "okay";
	current-speed = <115200>;
};
